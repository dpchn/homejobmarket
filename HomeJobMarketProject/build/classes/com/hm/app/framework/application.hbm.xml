<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.hm.app.model.Application" table="application">
		<id name="id" type="int">
			<column name="appId" />
			<generator class="increment" />
		</id>
		<!-- <property name="applyBy">
			<column name="apply_by" />
		</property>
		
		<property name="jobId" >
			<column name="job_id" />
		</property> -->
		
		<many-to-one name="applyBy" class="com.hm.app.model.User" column="apply_by" cascade="all" />
		<many-to-one name="jobId" class="com.hm.app.model.Job" column="job_id" cascade="all" />
	</class>
</hibernate-mapping>