<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.hm.app.model.RegisterModel" table="register">
		<id name="id" type="int">
			<column name="id" />
			<generator class="increment" />
		</id>
		<property name="fName" type="string">
			<column name="fname" length="20" not-null="true" />
		</property>
		<property name="lName" type="string">
			<column name="lname" />
		</property>
		<property name="phoneNo">
			<column name="phone" length="20" not-null="true" />
		</property>
		<property name="email" type="string">
			<column name="email" length="20" not-null="true" />
		</property>
		<property name="password" type="string">
			<column name="password" />
		</property>
		
		<property name="type">
			<column name="type" />
		</property>
		
		<property name="noOfChild">
			 <column name="noOfChild" />
		</property>

	</class>
</hibernate-mapping>